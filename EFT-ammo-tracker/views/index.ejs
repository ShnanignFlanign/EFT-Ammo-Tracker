<%-include('./partials/header.ejs')%>
<h1>Caliber List</h1>
    <div class="container">
<% for(let i = 0; i < calArr.length; i++) { %>
        <div class="col">
            <a class="btn btn-secondary" href="/ammo/<%=calArr[i].caliber%>"><%=calArr[i].caliber%></a>
                <ul class="list-group list-group-flush">
            <%for(let j = 0; j < calArr[i].bullets.length; j++){%>
                    <li class="list-group-item"><a class="btn btn-outline-secondary" href="/ammo/<%=calArr[i].caliber%>/<%=calArr[i].bullets[j].id%>"><%=calArr[i].bullets[j].cartridge%></a>
                    </li>
                <%}%>
                </ul>
        </div>
<%}%>
    </div>

<%- include('./partials/footer.ejs') %>